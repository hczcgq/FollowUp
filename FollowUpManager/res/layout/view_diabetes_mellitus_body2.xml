<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/diabetesMellitusRelativeLayout"
    android:paddingBottom="20dp" >

        <CheckBox
            android:id="@+id/TOD0"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:checked="true"
            android:text="无症状" />

        <CheckBox
            android:id="@+id/TOD1"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD0"
            android:text="多饮" />

        <CheckBox
            android:id="@+id/TOD2"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD1"
            android:text="多食" />

        <CheckBox
            android:id="@+id/TOD3"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD2"
            android:text="多尿" />

        <CheckBox
            android:id="@+id/TOD4"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD3"
            android:text="视力模糊" />

        <CheckBox
            android:id="@+id/TOD5"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD4"
            android:text="感染" />
        
        <CheckBox
            android:id="@+id/TOD6"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="手脚麻木" />
        
        <CheckBox
            android:id="@+id/TOD7"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD6"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="下肢浮肿" />
        <CheckBox
            android:id="@+id/TOD8"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD7"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="体重明显下降" />
        <CheckBox
            android:id="@+id/TOD9"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD8"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="消瘦" />
        <CheckBox
            android:id="@+id/TOD10"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD9"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="乏力" />
        <CheckBox
            android:id="@+id/TOD11"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD10"
            android:layout_below="@id/TOD0"
            android:layout_marginTop="10dp"
            android:text="手足疼痛" />
        <CheckBox
            android:id="@+id/TOD12"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_below="@id/TOD6"
            android:layout_marginTop="10dp"
            android:text="四肢发凉" />
        
        <CheckBox
            android:id="@+id/TOD13"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD12"
            android:layout_below="@id/TOD6"
            android:layout_marginTop="10dp"
            android:text="神经紧张" />
        <CheckBox
            android:id="@+id/TOD14"
            style="@style/CheckBoxStyle"
            android:layout_width="160dp"
            android:layout_toRightOf="@id/TOD13"
            android:layout_below="@id/TOD6"
            android:layout_marginTop="10dp"
            android:text="其他" />
        
        <EditText
            android:id="@+id/et_other"
            android:layout_width="220dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/TOD6"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/TOD14"
            android:background="@drawable/edit_text_with_padding_selector"
            android:gravity="center_vertical"
            android:textSize="@dimen/global_font_size_large" />

</RelativeLayout>